<mapper namespace="com.vertexid.paragon.file.FileLog.mysql">
    <insert id="insert" parameterType="com.vertexid.paragon.file.FileDownLogDTO">
        INSERT INTO t_sys_down_log (
            LOG_UID
            , DOWN_DTE
            , DOWN_LOGIN_ID
            , ATCH_NAME
            , ATCH_UID
            , FILE_SAVE_NM
            , ATCH_META
        ) VALUES (
            #{logUid}
            , NOW()
            , #{loginInfo.loginId}
            , #{atchName}
            , #{atchUid}
            , #{fileSaveNm}
            , #{atchMeta}
        )
    </insert>
</mapper>