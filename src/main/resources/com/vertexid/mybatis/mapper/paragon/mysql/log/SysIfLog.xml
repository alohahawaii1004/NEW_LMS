<!-- 인터페이스 로그 Mapper -->
<mapper namespace="com.vertexid.paragon.log.SysIfLog.mysql">

    <insert id="insert" parameterType="com.vertexid.paragon.security.SysIfLogDTO">
        INSERT INTO T_SYS_IF_LOG(
            IF_ID
            , IF_ITEM_CD
            , OPERT_ST_DTIME
            , OPERT_ED_DTIME
            , IF_RST_CD
            , OPERT_PARAM
            , OPERT_RST
            , OPERT_STAGE
            , RST_DTL
            , REG_DTE
            , REG_LOGIN_ID
            , UPT_DTE
            , UPT_LOGIN_ID
        )VALUES(
            #{ifId}
            , #{ifItemCd}
            , NOW()
            , NOW()
            , #{ifRstCd}
            , #{opertParam}
            , #{opertRst}
            , #{opertStage}
            , #{rstDtl}
            , NOW()
            , 'SYSTEM'
            , NOW()
            , 'SYSTEM'
        )
    </insert>

</mapper>