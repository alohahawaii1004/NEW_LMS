<mapper namespace="com.vertexid.paragon.log.AccLog.mysql">
	<insert id="insert" parameterType="map" >
		INSERT INTO t_sys_log_acc(
			 LOG_UID
			,ACTION_CD
			,LOG_TYPE_CD
			,REL_UID
			,DEPT_CD
			,DEPT_NM
			,LOGIN_ID
			,USER_NM
			,LOGIN_IP
			,REG_DTE
		)VALUES(
			 #{logUid}
			,#{actionCd}
			,#{logTypeCd}
			,#{relUid}
			,#{deptCd}
			,#{deptNm}
			,#{loginId}
			,#{userNm}
			,#{loginIp}
			,NOW()
		)
	</insert>
</mapper>